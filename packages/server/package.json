{
  "name": "@devops/server",
  "version": "1.0.0",
  "main": "dist/app.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app.js",
    "start-dev": "dotenv -e ./config/dev.env -- node dist/app.js",
    "dev": "nodemon --watch src/* --exec yarn nodemon-dev-script",
    "nodemon-dev-script": "yarn build && yarn start-dev",
    "build": "ttsc",
    "build-w": "ttsc -w",
    "prisma-push-dev": "dotenv -e ./config/dev.env -- npx prisma db push",
    "prisma-push-test": "dotenv -e ./config/test.env -- npx prisma db push",
    "prisma-push-prod": "npx prisma db push",
    "prisma-studio": "dotenv -e ./config/dev.env -- npx prisma studio",
    "test": "jest --watch",
    "write-secrets": "yarn ts-node helpers/writeSecrets.ts",
    "push-secrets": "kubectl apply -f ./kubernetes/secrets/graphql-secret.yml",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@paljs/plugins": "^3.8.0",

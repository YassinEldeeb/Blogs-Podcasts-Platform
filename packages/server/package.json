  "devDependencies": {
    "@types/archiver": "^5.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/colors": "^1.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/graphql-fields": "^1.3.4",
    "@types/graphql-upload": "^8.0.6",

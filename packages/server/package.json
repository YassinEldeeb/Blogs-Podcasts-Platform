    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dataloader": "^2.0.0",
    "dotenv-cli": "^4.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "extract-zip": "^2.0.1",
    "form-data": "^4.0.0",
    "googleapis": "^82.0.0",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.0.10",
    "graphql-query-complexity": "^0.8.1",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-scalars": "^1.10.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-upload": "^12.0.0",
    "ioredis": "^4.27.6",
    "js-base64": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mailparser": "^3.3.0",

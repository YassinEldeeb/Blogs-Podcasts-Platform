{
  "name": "graphql-basics",
  "version": "1.0.0",
  "main": "dist/app.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/app.js",
    "dev": "nodemon --watch dist/* --watch src/schema.graphql -e ts,js,graphql --exec yarn start",
    "genClientAndPushToDB": "concurrently \"prisma generate\" \"prisma db push\"",
    "gen-client": "nodemon --watch prisma/schema.prisma --exec yarn genClientAndPushToDB"
  },
  "dependencies": {
    "@paljs/plugins": "^3.6.0",
    "@prisma/client": "2.26.0",
    "@types/cookie-parser": "^1.4.2",
    "apollo-server-core": "^3.0.1",
    "apollo-server-express": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.0.10",
    "graphql-query-complexity": "^0.8.1",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-scalars": "^1.10.0",
    "graphql-subscriptions": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "subscriptions-transport-ws": "^0.9.19",
    "ts-node": "^10.0.0",
    "type-graphql": "^1.1.1",
    "typescript": "4.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/graphql-fields": "^1.3.4",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/node": "^15.14.0",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7",
    "prisma": "^2.27.0",
    "typegraphql-prisma": "^0.14.5"
  }
}

{
  "name": "graphql",
  "version": "1.0.0",
  "main": "dist/app.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app.js",
    "start-dev": "dotenv -e ./config/dev.env -- node dist/app.js",
    "dev": "nodemon --watch dist/* --exec yarn start-dev",
    "genClientAndPushToDB": "concurrently \"prisma generate\" \"prisma db push\"",
    "gen-client": "nodemon --watch prisma/schema.prisma --exec yarn genClientAndPushToDB",
    "build": "ttsc",
    "build-w": "ttsc -w",
    "prisma-push-dev": "dotenv -e ./config/dev.env -- npx prisma db push",
    "prisma-push-test": "dotenv -e ./config/test.env -- npx prisma db push",
    "prisma-push-prod": "dotenv -e ./config/prod.env -- npx prisma db push",
    "prisma-studio": "dotenv -e ./config/dev.env -- npx prisma studio",
    "test": "jest --watch"
  },
  "dependencies": {
    "@getvim/execute": "^1.0.0",
    "@prisma/client": "2.26.0",
    "apollo-server-core": "^3.0.1",
    "apollo-server-express": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "googleapis": "^82.0.0",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.0.10",
    "graphql-query-complexity": "^0.8.1",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-scalars": "^1.10.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-upload": "^12.0.0",
    "gzipme": "^1.0.0",
    "ioredis": "^4.27.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.6.3",
    "reading-time": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.28.3",
    "stream-to-buffer": "^0.1.0",
    "subscriptions-transport-ws": "^0.9.19",
    "ts-node": "^10.0.0",
    "type-graphql": "^1.1.1",
    "typescript": "4.3.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@paljs/plugins": "^3.6.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/graphql-fields": "^1.3.4",
    "@types/graphql-upload": "^8.0.6",
    "@types/ioredis": "^4.26.6",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/node": "^15.14.0",
    "@types/node-cron": "^2.0.4",
    "@types/nodemailer": "^6.4.4",
    "@types/sharp": "^0.28.4",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.2.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "prisma": "2.26.0",
    "ts-jest": "^27.0.4",
    "tsconfig-paths": "^3.10.1",
    "ttypescript": "^1.5.12",
    "typescript-transform-paths": "^3.1.0"
  }
}
